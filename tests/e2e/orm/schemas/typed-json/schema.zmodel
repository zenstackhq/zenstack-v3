datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
}

model User {
    id      Int     @id @default(autoincrement())
    profile Profile @json
}

enum Gender {
    MALE
    FEMALE
}

type Profile {
    age     Int
    name    String
    gender  Gender?
    jobs    Job[]
    address Address?
    tags    String[]?
}

type Address {
    country String
    zip     String?
}

type Job {
    title String
}
