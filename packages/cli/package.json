{
    "name": "@zenstackhq/cli",
    "publisher": "zenstack",
    "displayName": "ZenStack CLI",
    "description": "FullStack database toolkit with built-in access control and automatic API generation.",
    "version": "3.1.1",
    "type": "module",
    "author": {
        "name": "ZenStack Team"
    },
    "homepage": "https://zenstack.dev",
    "license": "MIT",
    "keywords": [
        "orm",
        "fullstack",
        "react",
        "typescript",
        "data modeling"
